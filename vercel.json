{
  "version": 2,
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "app.py"
    },
    {
      "src": "/favicon.ico",
      "dest": "app.py"
    },
    {
      "src": "/manifest.json",
      "dest": "app.py"
    },
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "functions": {
    "app.py": {
      "includeFiles": "static/**",
      "runtime": "@vercel/python"
    }
  },
  "env": {
    "FLASK_ENV": "production",
    "VERCEL": "1"
  }
}

